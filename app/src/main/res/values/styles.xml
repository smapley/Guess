<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="MyBaseAppTheme">
        <!-- Customize your theme here. -->
    </style>

    <style name="MyBaseAppTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/default_text</item>
        <item name="colorPrimaryDark">@color/default_text</item>
        <item name="colorAccent">@color/default_text</item>
        <item name="android:textColorPrimary">@color/default_text</item>
    </style>



    <style name="log_ev">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginEnd">60dp</item>
        <item name="android:layout_marginStart">60dp</item>
        <item name="android:background">@drawable/edittext_bg_select</item>
        <item name="android:lines">1</item>
        <item name="android:maxLength">20</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingEnd">20dp</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:textColorHint">@color/gray_text</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>

    <style name="log_bt">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="chose_item_layout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:padding">1dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/textview_circle</item>
    </style>

    <style name="chose_item">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:maxLines">2</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingRight">3dp</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="chose_item_clo">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@color/blue</item>
        <item name="android:lines">1</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="chose_et_item">
